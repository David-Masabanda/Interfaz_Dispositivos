<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.PruebasDispositivos" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/rojo</item>
    </style>

    <style name="Theme.PruebasDispositivos" parent="Base.Theme.PruebasDispositivos" />

    <style name="styleTitulo">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="styleSubtitulo">
        <item name="android:textColor">@color/low_gray</item>
    </style>
    <style name="styleLabel">
        <item name="android:textColor">@color/color_boton</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ActiveIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/low_gray</item>
    </style>


</resources>